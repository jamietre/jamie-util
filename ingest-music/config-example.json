{
  "libraryBasePath": "/path/to/library",
  "setlistSources": {
    "setlist.fm": {
      "apiKey": "your-setlist-fm-api-key",
      "url": "https://api.setlist.fm/rest/1.0"
    },
    "phish.net": {
      "apiKey": "your-phish-net-api-key",
      "url": "https://api.phish.net/v5"
    },
    "kglw.net": {
      "apiKey": "",
      "url": "https://kglw.net/api/v2"
    }
  },
  "llm": {
    "enabled": false,
    "provider": "ollama",
    "model": "qwen2.5:7b",
    "apiEndpoint": "http://127.0.0.1:11434",
    "maxTokens": 4000,
    "autoApply": false,
    "maxTokensPerRun": 50000
  },
  "webSearch": {
    "enabled": false,
    "provider": "brave",
    "apiKey": "your-brave-search-api-key",
    "maxResults": 10
  },
  "defaults": {
    "setlistSources": ["setlist.fm"],
    "albumTemplate": "{date} - {venue}, {location}",
    "albumArtist": "{artist}",
    "genre": "Live",
    "targetPathTemplate": "{artist}/{date} - {venue}, {location}",
    "fileNameTemplate": "{date} S{set} T{track} - {title}.flac",
    "fileNameTemplateSingleSet": "{date} T{track} - {title}.flac",
    "encoreInSet2": true,
    "conversion": {
      "flac": {
        "compressionLevel": 8
      }
    },
    "excludePatterns": ["^\\._", "^\\.DS_Store$", "^Thumbs\\.db$", "^\\."]
  },
  "bands": {
    "phish": {
      "name": "Phish",
      "patterns": ["^phish$"],
      "setlistSources": ["phish.net", "setlist.fm"],
      "genre": "Jam"
    },
    "kglw": {
      "name": "King Gizzard & The Lizard Wizard",
      "patterns": ["^king gizzard (& |and )?(the )?lizard wizard$", "^kglw$"],
      "setlistSources": ["kglw.net", "setlist.fm"],
      "genre": "Psychedelic Rock"
    }
  }
}
